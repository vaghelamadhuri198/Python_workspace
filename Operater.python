#Logical operater
x = 5
print(x > 3 and x < 10)
print(x > 3 or x < 10)
print(x > 3 != x < 10)

#Bitwise Operators
print(10 << 21)
print(10 >> 21)
print(x > 3 & x < 10)
print(~100)


