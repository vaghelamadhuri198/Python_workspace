#Arithmetic operater
a = 10
b = 3
print(a + b)
print(a - b)
print(a * b)
print(a / b)
print(a % b)
print(a**b)

#Realison operater

print(5 > 3)     # True
print(5 < 3)     # False
print(5 >= 3)   # True
print(5 <= 3)   # False
print(5 != 3)    # True

